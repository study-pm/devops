#!/bin/bash

### DESCRIPTION: Clears all of the output generated by running the start command

source VARS.sh
source TMP.sh

# Remove the containers
./rm-cont.sh "${LABEL}-test"

# Remove the images
./rm-img.sh "$REPO:v1"

# Remove the volumes
./rm-vol.sh $VOLUME

# Remove temporary files
rm TMP.sh

# List the Container (must be empty)
./list.sh

# Verify the Images (must be empty)
./verify.sh

# Listout Volumes
./listout.sh
