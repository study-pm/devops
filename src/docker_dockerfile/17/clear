#!/bin/bash

### DESCRIPTION: Clears all of the output generated by running the start command

source VARS.sh

# Remove the containers
./rm-cont.sh "${LABEL}-default"
./rm-cont.sh "${LABEL}-custom"

# Remove the images
./rm-img.sh "$NAMESPACE/$REPO:v1"
./rm-img.sh "$NAMESPACE/$REPO:v2"

# List the Containers (must be empty)
./list.sh

# Verify the Images (must be empty)
./verify.sh
