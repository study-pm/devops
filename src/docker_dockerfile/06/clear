#!/bin/bash

### DESCRIPTION: Clears all of the output generated by running the start command

source VARS.sh

# Remove the containers
./rm-cont.sh "${LABEL}-1"
./rm-cont.sh "${LABEL}-2"
./rm-cont.sh "${LABEL}-3"
./rm-cont.sh "${LABEL}-4"

# Remove the images
./rm-img.sh "$REPO:v1"
./rm-img.sh "$REPO:v2"
./rm-img.sh "$REPO:v3"
./rm-img.sh "$REPO:v4"

# List the Container (must be empty)
./list.sh

# Verify the Images (must be empty)
./verify.sh
